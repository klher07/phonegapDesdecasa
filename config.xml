<?xml version='1.0' encoding='utf-8'?>
<widget id="com.firebasepro.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>firebasepro</name>
    <description>
        Aplicacion Desdecasa
    </description>
    <author email="pestrella@global-andina.com" href="https://desdecasa.ec">
        Desdecasa
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/img/globaandina.png" />
    <preference name="android-minSdkVersion" value="19" />
</widget>
